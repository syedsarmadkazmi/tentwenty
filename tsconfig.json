{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
	"jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "~assets": ["assets"],
      "~config": ["src/config"],
      "~screens": ["src/screens"],
      "~navigation": ["src/navigation"],
      "~types": ["src/types"],
      "~services": ["src/services"],
      "~theme": ["src/theme"],
      "~components": ["src/components"],
      "~redux": ["src/redux"],
      "~apis": ["src/apis"],
    },
  },
  "include": ["**/*.ts", "**/*.tsx"],
}
